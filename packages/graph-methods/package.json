{
  "name": "@alpaca-travel/graph-methods",
  "version": "0.0.14-alpha.0",
  "description": "Simple GraphQL queries and mutations exposed as JS API",
  "homepage": "https://github.com/AlpacaTravel/graph-methods",
  "repository": {
    "type": "git",
    "url": "git://github.com/AlpacaTravel/graph-sdk.git"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn run type-check -- --watch",
    "test": "jest",
    "test:coverage": "yarn build & jest --coverage",
    "clean": "rm -rf dist",
    "build": "yarn run build:rollup",
    "build:rollup": "rollup -c",
    "pretest": "yarn build",
    "prepublish": "yarn build"
  },
  "env": {
    "jest": true
  },
  "files": [
    "dist"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!<rootDir>/node_modules/"
    ]
  },
  "author": "Cameron Manderson <cam@alpaca.travel>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.9.2",
    "@babel/types": "^7.9.6",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "fetch-mock": "^9.4.0",
    "jest": "^25.5.0",
    "rollup": "^2.7.3",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.27.0",
    "typescript": "^3.8.3"
  }
}
