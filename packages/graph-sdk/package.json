{
  "name": "@alpaca-travel/graph-sdk",
  "version": "0.0.4-alpha.0",
  "description": "Alpaca Travel Graph SDK Client",
  "homepage": "https://github.com/AlpacaTravel/graph-sdk",
  "repository": {
    "type": "git",
    "url": "git://github.com/AlpacaTravel/graph-sdk.git"
  },
  "main": "dist/index.cjs.js",
  "browser": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "yarn build & jest --coverage",
    "clean": "rm -rf dist",
    "build": "yarn run rollup & yarn run webpack",
    "rollup": "rollup -c",
    "webpack": "webpack",
    "pretest": "yarn build",
    "postpublish": "yarn release",
    "release": "PUBLISHED=true yarn build && PUBLISHED=true bin/deploy"
  },
  "env": {
    "jest": true
  },
  "files": [
    "dist"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "dist/**/*.cjs.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/dist/bundle.js",
      "!<rootDir>/src/index.js"
    ]
  },
  "author": "Cameron Manderson <cam@alpaca.travel>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.9.2",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.5.0",
    "rollup": "^2.7.3",
    "rollup-plugin-terser": "^5.1.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-node-externals": "^1.7.2"
  }
}
